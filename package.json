{
  "name": "astra-pos",
  "author": "FMT",
  "licence": "MIT",
  "version": "1.0.0",
  "private": false,
  "main": "build/main.js",
  "scripts": {
    "start-react": "PORT=9000 BROWSER=none react-scripts start",
    "startwin-react": "react-scripts start",
    "build-react": "react-scripts build",
    "start-electron": "nodemon --watch ./public/* --exec \"electron .\"",
    "start": " cp main.js ./public/main.js &&  concurrently \"npm run start-react\" \"wait-on http://localhost:9000 && npm run start-electron\" ",
    "start:win": "concurrently \"npm run startwin-react\" \"wait-on http://localhost:3000 && npm run start-electron\" ",
    "postinstall": "electron-builder install-app-deps",
    "pack-app": "npm run build-react && electron-builder -mwl --dir",
    "pack-build": "electron-builder --dir",
    "build": "cp main.js ./public/main.js && cp main.js ./build/main.js && npm run build-react && electron-builder -mw",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "rebuild-arm64": "electron-rebuild -f --arch=arm64",
    "install-sqlite": "npm install sqlite3@5.0.0 --build-from-source --runtime=electron --target=9.4.4 --dist-url=https://electronjs.org/headers --target_arch=ia32 &&  npm install sqlite3@5.0.0 --build-from-source --runtime=electron --target=9.4.4 --dist-url=https://electronjs.org/headers --target_arch=x64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "crypto": false
  },
